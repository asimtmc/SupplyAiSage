{
  "name": "AI Supply Chain Platform",
  "description": "Advanced AI-powered supply chain forecasting and analysis tool",
  "repository": "https://github.com/yourusername/supply-chain-forecasting",
  "keywords": ["python", "streamlit", "supply-chain", "forecast", "inventory"],
  "stack": "heroku-24",
  "formation": {
    "web": {
      "quantity": 1,
      "size": "standard-1x"
    }
  },
  "env": {
    "DISABLE_UV_LOCKING": {
      "description": "Disable UV lockfile requirement",
      "value": "true"
    },
    "DISABLE_COLLECTSTATIC": {
      "description": "Disable collectstatic during build",
      "value": "1"
    },
    "TF_CPP_MIN_LOG_LEVEL": {
      "description": "Suppress TensorFlow warnings",
      "value": "2"
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-apt"
    },
    {
      "url": "heroku/python"
    }
  ]
}